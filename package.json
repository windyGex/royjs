{
  "name": "roy.js",
  "version": "0.2.1",
  "description": "A tiny mvvm library for react",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "build:umd": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js",
    "prepublish": "npm run build && npm run build:umd",
    "example:todo": "parcel examples/todo-mvc/index.html",
    "example:counter": "parcel examples/counter/index.html",
    "example:scene": "parcel examples/scenes/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/windyGex/roy.git"
  },
  "files": [
    "dist",
    "docs",
    "lib",
    "test",
    "HISTORY.md",
    "README.md",
    "index.js",
    "router.js",
    "request.js"
  ],
  "keywords": [
    "mvvm",
    "react",
    "tiny",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "xing.gex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windyGex/roy/issues"
  },
  "homepage": "https://github.com/windyGex/roy",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.6",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "jsdom": "^11.12.0",
    "mocha": "^4.0.1",
    "parcel": "^1.9.7",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react-router-dom": "^4.3.1"
  }
}
